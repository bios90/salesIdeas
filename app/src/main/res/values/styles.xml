    <style name="Custom" parent="android:Theme.DeviceDefault.Dialog">
        <item name="DialogTitleAppearance">@style/dialogText</item>
        <item name="DialogSpotColor">@color/myLightGray</item>
        <item name="android:background">@color/myBlack</item>
        <item name="DialogSpotCount">5</item>
    </style>

    <style name="dialogText" parent="@android:style/TextAppearance.Medium">
