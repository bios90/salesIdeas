<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/myPink</item>
        <item name="colorPrimaryDark">@color/myPink</item>
        <item name="colorAccent">@color/myPink</item>

        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:statusBarColor">@android:color/transparent</item>


        <item name="android:spinnerItemStyle">
            @style/spinnerItemStyle
        </item>

        <item name="android:spinnerDropDownItemStyle">
            @style/spinnerDropDownItemStyle
        </item>
    </style>

    <style name="DatePickerSpinner">
        <item name="android:textSize">22dp</item>
        <item name="android:textColorPrimary">@color/myPink</item>
        <item name="android:colorControlNormal">@color/myPink</item>
    </style>

    <style name="spinnerItemStyle">
        <item name="android:fontFamily">@font/monsemibold</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/myBlack</item>
    </style>

    <style name="spinnerDropDownItemStyle">
        <item name="android:fontFamily">@font/monsemibold</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/myBlack</item>
    </style>

